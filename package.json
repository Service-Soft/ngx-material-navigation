{
    "name": "ngx-material-navigation",
    "version": "0.0.0",
    "scripts": {
        "start": "npm run stack",
        "stack": "run-s clear:dist stack:only",
        "showcase": "ng serve ngx-material-navigation-showcase",
        "stack:only": "concurrently --kill-others 'npm run build:watch' 'run-s waitOn:build showcase'",
        "clear:dist": "rimraf dist/ngx-material-navigation",
        "waitOn:build": "wait-on dist/ngx-material-navigation",
        "waitOn:server": "wait-on http://localhost:4200",
        "build": "ng build ngx-material-navigation",
        "build:watch": "ng build ngx-material-navigation --watch",
        "build:prod": "run-s build copyToDist",
        "copyToDist": "run-s copyToDist:readme copyToDist:contributing copyToDist:license",
        "copyToDist:readme": "ncp ./README.md ./dist/ngx-material-navigation/README.md",
        "copyToDist:contributing": "ncp ./CONTRIBUTING.md ./dist/ngx-material-navigation/CONTRIBUTING.md",
        "copyToDist:license": "ncp ./LICENSE.md ./dist/ngx-material-navigation/LICENSE.md",
        "test": "ng test",
        "lint": "eslint projects --max-warnings 0",
        "lint:fix": "eslint projects --fix"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.3.0",
        "@angular/cdk": "^13.3.9",
        "@angular/common": "~13.3.0",
        "@angular/compiler": "~13.3.0",
        "@angular/core": "~13.3.0",
        "@angular/forms": "~13.3.0",
        "@angular/material": "^13.3.9",
        "@angular/platform-browser": "~13.3.0",
        "@angular/platform-browser-dynamic": "~13.3.0",
        "@angular/router": "~13.3.0",
        "bootstrap": "^5.1.3",
        "dompurify": "^2.3.10",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.3.9",
        "@angular-eslint/builder": "13.5.0",
        "@angular-eslint/eslint-plugin": "13.5.0",
        "@angular-eslint/eslint-plugin-template": "13.5.0",
        "@angular-eslint/schematics": "13.5.0",
        "@angular-eslint/template-parser": "13.5.0",
        "@angular/cli": "~13.3.5",
        "@angular/compiler-cli": "~13.3.0",
        "@cspell/eslint-plugin": "^6.4",
        "@types/jasmine": "~3.10.0",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "5.27.1",
        "@typescript-eslint/parser": "5.27.1",
        "@types/dompurify": "^2.3.3",
        "concurrently": "^7.2.2",
        "eslint": "^8.17.0",
        "eslint-config-service-soft": "^1.0.2",
        "eslint-plugin-jsdoc": "^39.3.3",
        "jasmine-core": "~4.0.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "ncp": "^2.0.0",
        "ng-packagr": "^13.0.0",
        "npm-run-all": "^4.1.5",
        "typescript": "~4.6.2",
        "wait-on": "^6.0.1"
    }
}